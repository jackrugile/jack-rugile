-
  const pageTitle = `Page Not Found | ${data.name}`
  const pageURL = `${data.baseUrl}/404`

doctype html
html.is-404(
  lang="en"
  itemscope
  itemtype="http://schema.org/WebPage"
)
  include ./includes/head.pug
  body
    div.page-not-found
      include ./includes/svgs/angle-top.pug
      include ./includes/svgs/angle-bot.pug
      div.page-not-found-title
        h1.page-not-found-title-text Page Not Found
