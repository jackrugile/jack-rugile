head
  meta(charset="utf-8")
  meta(
    name="viewport"
    content="width=device-width, initial-scale=1, viewport-fit=cover"
  )
  title #{pageTitle}
  meta(
    name="description"
    content=""
  )
  meta(
    name="author"
    content=data.name
  )
  link(
    href="https://fonts.googleapis.com/css?family=Merriweather:300,300i,400,400i,700,700i,900,900i&font-display=swap"
    rel="stylesheet"
  )
  link(
    href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i&font-display=swap"
    rel="stylesheet"
  )
  link(
    href="./styles/index.css"
    rel="stylesheet"
  )
  link(
    rel="manifest"
    href="./site.webmanifest"
  )
  link(
    rel="apple-touch-icon"
    sizes="180x180"
    href="./icons/apple-touch-icon.png"
  )
  link(
    rel="icon"
    type="image/png"
    sizes="32x32"
    href="./icons/favicon-32x32.png"
  )
  link(
    rel="icon"
    type="image/png"
    sizes="16x16"
    href="./icons/favicon-16x16.png"
  )
  link(
    rel="mask-icon"
    href="./icons/safari-pinned-tab.svg"
    color="#ff0040"
  )
  meta(
    name="msapplication-TileColor"
    content="#ffffff"
  )
  meta(
    name="msapplication-config"
    content="./browserconfig.xml"
  )
  meta(
    name="theme-color"
    content="#ffffff"
  )
  meta(
    name="apple-mobile-web-app-capable"
    content="yes"
  )
  meta(
    name="apple-mobile-web-app-status-bar-style"
    content="black"
  )
  link(
    rel="canonical"
    href=pageURL
  )
